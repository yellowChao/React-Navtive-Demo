{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "es6": true,
    "jasmine": true
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "eslint:recommended",
    "airbnb"
  ],
  "rules": {
    "indent": [
      2,
      2
    ],
    "id-length": 0,
    "max-len": 0,
    "semi": 0,
    "object-curly-spacing": 0,
    "object-shorthand": 0,
    "no-use-before-define": 0,
    "no-console": 0,
    "no-ternary": 0,
    "react/sort-comp": [0],
    "react/prefer-stateless-function": 0,
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
